RewriteEngine on

# redirect www to non-www
#RewriteCond %{HTTP_HOST} ^www\.notebookdd\.com$ [NC]
#RewriteRule ^(.*)$ http://notebookdd.com/$1 [L,R=301]

RewriteCond %{HTTP_HOST} ^notebookdd\.com$ [NC]
RewriteRule ^(.*)$ http://www.notebookdd.com/$1 [L,R=301]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule .* index.php/$0 [PT,L]
